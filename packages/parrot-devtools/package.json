{
  "name": "parrot-devtools",
  "version": "4.1.0",
  "private": true,
  "contributors": [
    "Jack Cross <jack.cross1@aexp.com>",
    "Nathan Force <nathan.a.force@aexp.com>",
    "Jason Schapiro",
    "Zach Ripka"
  ],
  "description": "Devtools for switching Parrot scenarios.",
  "scripts": {
    "start": "chokidar 'src/**' -c 'npm run build' --initial",
    "clean": "rimraf build && rimraf dist",
    "prebuild": "npm run clean",
    "build": "npm run bundle",
    "bundle": "webpack -p && npm run pack:extension && npm run pack:base",
    "pack:extension": "node scripts/pack-extension.js",
    "pack:base": "node scripts/pack-base.js"
  },
  "license": "Apache-2.0",
  "repository": "americanexpress/parrot",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "styled-components": "^4.4.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "archiver": "^2.1.1",
    "babel-loader": "^8.0.6",
    "babel-preset-amex": "^3.3.0",
    "chokidar": "^3.3.1",
    "chokidar-cli": "^2.1.0",
    "file-loader": "^5.0.2",
    "fs-extra": "^2.1.2",
    "import-glob-loader": "^1.1.0",
    "rimraf": "^3.0.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.10"
  }
}
