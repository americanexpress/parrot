{
  "name": "parrot",
  "version": "3.0.0",
  "scripts": {
    "postinstall": "lerna bootstrap",
    "clean": "git clean -d -X",
    "test": "jest",
    "pretest": "npm run lint",
    "lint": "eslint --ext js,jsx ./",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/americanexpress/parrot.git"
  },
  "contributors": [
    "Jack Cross <jack.cross1@aexp.com>",
    "Nathan Force <nathan.a.force@aexp.com>",
    "Jason Schapiro"
  ],
  "devDependencies": {
    "amex-jest-preset-react": "^4.0.0",
    "babel-cli": "^6.7.5",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-amex": "^1.1.0",
    "babel-register": "^6.7.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.18.2",
    "eslint-config-amex": "^6.0.0",
    "eslint-config-prettier": "^2.9.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^21.0.0",
    "lerna": "2.1.2",
    "lint-staged": "^7.0.0",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.2.8",
    "whatwg-fetch": "^2.0.3"
  },
  "license": "Apache-2.0",
  "jest": {
    "preset": "amex-jest-preset-react",
    "collectCoverageFrom": [
      "packages/*/src/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.png": "identity-obj-proxy"
    },
    "setupFiles": [
      "./jest-setup.js"
    ]
  }
}
